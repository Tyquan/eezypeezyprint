extends ../layout

block content
  div(class="container")
    include ../partials/employeenav.pug
    div(id="dashboard")
      h1 Contacts
      table(class="table table-bordered table-striped table-hover table-condensed")
        tr
          th Name
          th Email
          th Phone Number
          th Reason
          th Message
        each contact in contacts
          tr
            td= contact.name
            td= contact.email
            td= contact.phoneNumber
            td= contact.reason
            td= contact.message